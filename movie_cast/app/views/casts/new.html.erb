<h1>Add a new movie and Cast!</h1>

<%= link_to "Back to Movie Casts", casts_url %>


<%= form_for @movie do |f|%>
  <p>
    <%= f.label :movie_title %><br>
    <%= f.text_field :movie_title %>
  </p>
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
